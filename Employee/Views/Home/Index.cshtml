@model  IEnumerable<EmployeeTable>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Employees</h4>
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <th>EmployeeID</th>
            <th>Name</th>
            <th>Country</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.EmployeeId</td>
                <td>@item.Name</td>
                <td>@item.Country</td>
            </tr>
        }
    </table>
</body>
</html>
