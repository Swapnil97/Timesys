@model IEnumerable<OdeToFood.Data.Models.Restaurant>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET Mvc In memory data</h1>
    <div>Using In Memory Data </div>
    <div>There are @Enumerable.Count(Model) number of restaurants in the db.  </div>
    <div>There are @Model.Count() restaurants here</div>
</div>

<ul>
    @foreach (var restaurant in Model) {
        <li>@restaurant.Name</li>
    }
</ul>